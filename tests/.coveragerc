[run]
omit =
    examples/app/app.py
    # 20200404 pytest-flask-sqlalchemy does not support sqlalchemy 1.4 yet
    examples/app/api.py
    open_alchemy/types.py
branch = True

[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain about missing debug-only code:
    def __repr__
    if self\.debug

    # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError

    # Don't complain if non-runnable code isn't run:
    if 0:
    if __name__ == .__main__.:
    ^        \.\.\.$

    # Don't complain about print statements not run
    print\(.*\)

    # Don't complain about typing_extension imports
    typing_extension
    # Don't complain about compatibility code
    sys.version_info
    version compatibility
skip_covered=true
show_missing=true
fail_under=100
